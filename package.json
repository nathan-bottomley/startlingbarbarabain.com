{
  "name": "startlingbarbarabain.com",
  "version": "1.0.0",
  "description": "The website for the podcast Startling Barbara Bain",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve",
    "episodes": "rclone sync episodes/ digitalocean:startlingbarbarabain -P --exclude .DS_Store",
    "postepisodes": "node script/episodeInfo.js",
    "stats": "node script/podcastInfo.js",
    "clean": "rm -rf _site"
  },
