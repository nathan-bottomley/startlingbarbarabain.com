    "@11tyrocks/eleventy-plugin-lightningcss": "^1.3.0",
    "@tsmx/human-readable": "^2.0.2",
    "browserslist": "^4.22.1",
    "chalk": "^5.3.0",
    "lightningcss": "^1.22.0",
    "luxon": "^3.4.3",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6",
    "mp3-duration": "^1.1.0",
    "standard": "^17.1.0"
